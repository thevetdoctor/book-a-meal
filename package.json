{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "a meal booking app for customers and vendors",
  "main": "server.js",
  "nyc": {
    "require": [
      "@babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
